        <mxCell id="LAFkxnOpKaz8uj2J0HjF-13" value="&lt;b&gt;CONVERSIÓN AL &lt;br&gt;M. FÍSICO&lt;/b&gt;" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#b1ddf0;strokeColor=#10739e;" parent="1" vertex="1">
